<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" vgap="10" xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.muhammed.sabry.Controller">
   <columnConstraints>
      <ColumnConstraints />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints />
      <RowConstraints />
      <RowConstraints minHeight="10.0" prefHeight="30.0" />
   </rowConstraints>
   <children>
       <ScrollPane fx:id="scrollPane" prefHeight="500.0" prefWidth="1000.0" GridPane.rowIndex="1">
        <content>
            <AnchorPane fx:id="tree_pane" minHeight="0.0" minWidth="0.0">
                <children>
                    <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true"
                               preserveRatio="true"/>
                </children>
            </AnchorPane>
        </content>
         <GridPane.margin>
            <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
         </GridPane.margin>
      </ScrollPane>
       <TextField fx:id="textField" editable="false" prefWidth="300.0" promptText="click to choose file">
         <GridPane.margin>
             <Insets left="8.0" right="8.0"/>
         </GridPane.margin></TextField>
       <Button fx:id="drawButton" contentDisplay="CENTER" minHeight="26.0" minWidth="400.0" mnemonicParsing="false"
               text="Draw" GridPane.halignment="CENTER" GridPane.rowIndex="2" GridPane.valignment="CENTER"/>
   </children>
   <padding>
      <Insets bottom="8.0" left="8.0" right="8.0" top="8.0" />
   </padding>
</GridPane>
